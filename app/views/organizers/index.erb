<h1>Event Organizer: <%= @organizer.name %></h1>

<h2>Your Events Posted to Our Website</h2><br>

<ul>
  <% @events.each do |event| %> 
	<li>
	  <h3><a href="/events/#{event.id}"><%= event.name %></a></h3>
	  <p><%= event.date %></p><br>
	</li>
  <% end %>
 </ul>

 <p><a href="/events/index">All Events</a></p>